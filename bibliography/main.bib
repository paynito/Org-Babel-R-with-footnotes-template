	Issn = {0276-5624},
	Journal = {Research in Social Stratification and Mobility},
	Keywords = {Race},
	Number = {4},
	Pages = {375 - 394},
	Title = {The impact of intermarriage on ethnic stratification: Jews in Israel},
	Url = {http://www.sciencedirect.com/science/article/pii/S0276562410000442},
	Volume = {28},
	Year = {2010},
